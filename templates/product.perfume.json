{
  "sections": {
    "main": {
      "type": "static-product",
      "blocks": {
        "title": {
          "type": "title",
          "settings": {
            "show_border": false
          }
        },
        "price": {
          "type": "price",
          "settings": {
            "show_border": true
          }
        },
        "c25c137a-5800-4b0a-86cb-fc8237f231a2": {
          "type": "sku",
          "settings": {
            "show_border": false
          }
        },
        "cdf83a70-a9b2-4468-b99d-be1f320f519b": {
          "type": "custom-liquid",
          "settings": {
            "custom_liquid": "{% assign current_variant = product.selected_or_first_available_variant %}\n\n<span class=\"product__barcode\">\n<strong data-orginal-font-size=\"12\" data-orginal-line-height=\"16.8\" data-orginal-letter-spacing=\"0\" style=\"font-size: 12px;\">EAN<\/strong> \n\n<spandata-orginal-font-size=\"12\" data-orginal-line-height=\"16.8\" data-orginal-letter-spacing=\"0\" style=\"font-size: 12px;\"><span class=\"variant-barcode\">{{ current_variant.barcode }}<\/span><\/span>"
          }
        },
        "form": {
          "type": "form",
          "settings": {
            "show_border": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "show_border": true
          }
        },
        "description": {
          "type": "description",
          "settings": {
            "show_border": false
          }
        },
        "fcdfff86-37d3-4b69-9c05-fb9f23fa010b": {
          "type": "collapsible-tab",
          "settings": {
            "collapsible_tab_heading": "OLFACTORY  PYRAMID",
            "collapsible_tab_text": "<p>{{ product.metafields.custom.olfactory_pyramid | metafield_tag }}<\/p>"
          }
        },
        "91d654f5-70fe-4a1f-b002-42e5c0209bc1": {
          "type": "collapsible-tab",
          "settings": {
            "collapsible_tab_heading": "OLFACTORY CHARM",
            "collapsible_tab_text": "<p>{{ product.metafields.custom.olfactory_charm | metafield_tag }}<\/p>"
          }
        },
        "47382f2b-f28a-45b0-a69a-b1fb0abe856d": {
          "type": "collapsible-tab",
          "settings": {
            "collapsible_tab_heading": "INGREDIENTS",
            "collapsible_tab_text": "<p>{{ product.metafields.custom.ingredients | metafield_tag }}<\/p>"
          }
        }
      },
      "block_order": [
        "title",
        "price",
        "c25c137a-5800-4b0a-86cb-fc8237f231a2",
        "cdf83a70-a9b2-4468-b99d-be1f320f519b",
        "form",
        "share",
        "description",
        "fcdfff86-37d3-4b69-9c05-fb9f23fa010b",
        "91d654f5-70fe-4a1f-b002-42e5c0209bc1",
        "47382f2b-f28a-45b0-a69a-b1fb0abe856d"
      ],
      "settings": {
        "details_alignment": "left",
        "show-breadcrumbs": false,
        "gallery_layout": "gallery-left",
        "gallery_enable_image_zoom": true,
        "gallery_enable_video_autoplay": true,
        "gallery_enable_video_looping": true
      }
    },
    "product-recommendations": {
      "type": "static-product-recommendations",
      "settings": {
        "show_product_recommendations": true,
        "product_recommendations_heading": "You may also like"
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
